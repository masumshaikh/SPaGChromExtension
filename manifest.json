{
	"manifest_version": 3,
	
	"name": "Simple Password Generator",
	
	"description": "Simple way to generate complex passwords which do not need to be stored anywhere, yet are easily recoverable.",
	
	"version": "1.5",
	
	"permissions": [ "tabs", "storage" ],
	
	"icons":
	{
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	
	"options_page": "options.html",
	
	"action":
	{
		"default_icon": "icons/icon19.png",
		"default_popup": "popup.html"
	}
	"content_security_policy": {}
}
